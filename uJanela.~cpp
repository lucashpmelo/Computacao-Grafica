//---------------------------------------------------------------------------


#pragma hdrstop
#include "uJanela.h"

//---------------------------------------------------------------------------

#pragma package(smart_init)

Janela::Janela (double xcMin,double ycMin,
                double xcMax,double ycMax){
    xMin=xcMin;
    xMax=xcMax;
    yMin=ycMin;
    yMax=ycMax;

}